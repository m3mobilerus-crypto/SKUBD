<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
фильтрации SKU<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
filterSKU() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
model = modelSelect.value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
android = androidSelect.value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
scanner = scannerSelect.value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
memory = memorySelect.value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
buttons = buttonsSelect.value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
handle = handleSelect.value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const lte
= lteSelect.value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
frost = frostSelect.value;<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
filteredData = skuData.filter(item =&gt; {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(model &amp;&amp; item.model !== model) return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(android &amp;&amp; item.android !== android) return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(scanner &amp;&amp; item.scanner !== scanner) return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(memory &amp;&amp; item.memory !== memory) return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(buttons &amp;&amp; item.buttons !== buttons) return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(lte === 'yes' &amp;&amp; !item.lte) return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(lte === 'no' &amp;&amp; item.lte) return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(frost === 'yes' &amp;&amp; item.frost !== 'yes') return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(frost === 'no' &amp;&amp; item.frost === 'yes') return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Исправленная фильтрация для типа рукоятки<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(handle === 'integrated' &amp;&amp; item.handle !== 'integrated' &amp;&amp;
item.handle !== 'интегрирована') return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(handle === 'separate' &amp;&amp; (item.handle === 'integrated' || item.handle
=== 'интегрирована')) return false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>return
true;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>});<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>displayResults(filteredData);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
отображения результатов<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
displayResults(data, searchTerm = '') {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>resultsContainer.innerHTML = '';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(data.length === 0) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>resultsContainer.innerHTML = `&lt;div
class=&quot;no-results&quot;&gt;${translations[currentLanguage]['no_results']}&lt;/div&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>return;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>data.forEach(item =&gt; {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>const
resultCard = document.createElement('div');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>resultCard.className = 'result-card';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Проверяем, добавлен ли уже этот товар в сравнение<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>const
isInComparison = comparisonItems.some(i =&gt; i.sku === item.sku);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>let
detailsHtml = `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>&lt;div class=&quot;result-header&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;sku-info-container&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;sku-info&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span
style='mso-spacerun:yes'>                                </span>&lt;div
class=&quot;sku-code&quot;&gt;${highlightSearchTerm(item.sku,
searchTerm)}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span
style='mso-spacerun:yes'>                                </span>&lt;div
class=&quot;sku-price&quot;&gt;${translations[currentLanguage]['price']}:
$${item.price.toFixed(2)}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>&lt;div class=&quot;sku-details&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;detail-item&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div
class=&quot;detail-label&quot;&gt;${translations[currentLanguage]['model']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;detail-value&quot;&gt;${item.model}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(item.android &amp;&amp; item.android !== 'NO') {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;detail-item&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div
class=&quot;detail-label&quot;&gt;${translations[currentLanguage]['android']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;detail-value&quot;&gt;${item.android}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(item.scanner) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;detail-item&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div
class=&quot;detail-label&quot;&gt;${translations[currentLanguage]['scanner']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;detail-value&quot;&gt;${item.scanner}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(item.memory) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;detail-item&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div
class=&quot;detail-label&quot;&gt;${translations[currentLanguage]['memory']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;detail-value&quot;&gt;${item.memory}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(item.buttons) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;detail-item&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div
class=&quot;detail-label&quot;&gt;${translations[currentLanguage]['buttons']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;detail-value&quot;&gt;${item.buttons}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(item.lte) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;detail-item&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div
class=&quot;detail-label&quot;&gt;${translations[currentLanguage]['lte']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;detail-value&quot;&gt;${item.lte}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(item.frost) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;detail-item&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div
class=&quot;detail-label&quot;&gt;${translations[currentLanguage]['frost']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;detail-value&quot;&gt;${item.frost === 'yes' ?
translations[currentLanguage]['frost_resistant'] :
translations[currentLanguage]['not_frost_resistant']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(item.handle) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;div class=&quot;detail-item&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div
class=&quot;detail-label&quot;&gt;${translations[currentLanguage]['handle']}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;div class=&quot;detail-value&quot;&gt;${item.handle === 'integrated'
|| item.handle === 'интегрирована' ? 'Интегрированная' :
'Отдельная'}&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>detailsHtml += `&lt;/div&gt;&lt;/div&gt;`; // закрываем sku-details и
sku-info-container<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Добавляем изображение модели справа<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>&lt;div class=&quot;sku-image-container&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;img src=&quot;${modelImages[item.model]}&quot;
alt=&quot;${item.model}&quot; class=&quot;sku-image&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>&lt;/div&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>detailsHtml += `&lt;/div&gt;`; // закрываем result-header<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Добавляем кнопку сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>&lt;button class=&quot;compare-button ${isInComparison ? 'added' :
''}&quot; onclick=&quot;toggleComparison('${item.sku}')&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>${isInComparison ? translations[currentLanguage]['remove_from_compare']
: translations[currentLanguage]['add_to_compare']}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>&lt;/button&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Добавляем ссылку на презентацию аксессуаров, если она есть для этой модели<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(accessoriesLinks[item.model]) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>detailsHtml += `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;a href=&quot;${accessoriesLinks[item.model]}&quot;
target=&quot;_blank&quot; class=&quot;accessories-link&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>${translations[currentLanguage]['accessories']} ${item.model}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;/a&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>detailsHtml += `&lt;div
class=&quot;description&quot;&gt;${highlightSearchTerm(item.description,
searchTerm)}&lt;/div&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>resultCard.innerHTML = detailsHtml;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>resultsContainer.appendChild(resultCard);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция для
подсветки искомого термина<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
highlightSearchTerm(text, searchTerm) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(!searchTerm) return text;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
regex = new RegExp(`(${searchTerm})`, 'gi');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>return
text.replace(regex, '&lt;span class=&quot;highlight&quot;&gt;$1&lt;/span&gt;');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
добавления/удаления товара из сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
toggleComparison(sku) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const item
= skuData.find(i =&gt; i.sku === sku);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Проверяем, есть ли уже этот товар в сравнении<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
index = comparisonItems.findIndex(i =&gt; i.sku === sku);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if (index
=== -1) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Добавляем товар в сравнение<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>comparisonItems.push(item);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>} else {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Удаляем товар из сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>comparisonItems.splice(index, 1);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Обновляем отображение кнопок сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>updateComparisonButtons();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Обновляем панель сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>updateComparisonPanel();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
обновления кнопок сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
updateComparisonButtons() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
buttons = document.querySelectorAll('.compare-button');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>buttons.forEach(button =&gt; {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>const
sku = button.getAttribute('onclick').replace(&quot;toggleComparison('&quot;,
&quot;&quot;).replace(&quot;')&quot;, &quot;&quot;);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>const
isInComparison = comparisonItems.some(i =&gt; i.sku === sku);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(isInComparison) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>button.classList.add('added');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>button.textContent =
translations[currentLanguage]['remove_from_compare'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>} else
{<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>button.classList.remove('added');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>button.textContent = translations[currentLanguage]['add_to_compare'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
обновления панели сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
updateComparisonPanel() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>comparisonItemsContainer.innerHTML = '';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(comparisonItems.length &gt; 0) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>comparisonPanel.classList.add('visible');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>comparisonItems.forEach(item =&gt; {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>const comparisonItem = document.createElement('div');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>comparisonItem.className = 'comparison-item';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>comparisonItem.innerHTML = `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>${item.sku}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;button class=&quot;remove-comparison&quot;
onclick=&quot;toggleComparison('${item.sku}')&quot;&gt;×&lt;/button&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>comparisonItemsContainer.appendChild(comparisonItem);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>} else {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>comparisonPanel.classList.remove('visible');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
очистки списка сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
clearComparison() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>comparisonItems = [];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>updateComparisonPanel();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>updateComparisonButtons();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
отображения модального окна сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
showComparison() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(comparisonItems.length &lt; 2) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>alert('Выберите как минимум два товара для сравнения');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>return;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>comparisonResults.innerHTML = '';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>// Создаем
таблицу сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>let
tableHtml = `<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>&lt;table class=&quot;comparison-table&quot;&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>&lt;thead&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>&lt;tr&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                           
</span>&lt;th&gt;Характеристика&lt;/th&gt;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Добавляем заголовки для каждого товара<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>comparisonItems.forEach(item =&gt; {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>tableHtml += `&lt;th&gt;${item.sku}&lt;/th&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>tableHtml
+= `&lt;/tr&gt;&lt;/thead&gt;&lt;tbody&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Определяем список характеристик для сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
properties = [<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'model', label: translations[currentLanguage]['model'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'android', label: translations[currentLanguage]['android'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'scanner', label: translations[currentLanguage]['scanner'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'memory', label: translations[currentLanguage]['memory'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'buttons', label: translations[currentLanguage]['buttons'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'handle', label: translations[currentLanguage]['handle'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'lte', label: translations[currentLanguage]['lte'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'frost', label: translations[currentLanguage]['frost'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'price', label: translations[currentLanguage]['price'] },<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>{ key:
'description', label: 'Описание' }<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Добавляем строки с характеристиками<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>properties.forEach(prop =&gt; {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Получаем значения для каждого товара<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>const
values = comparisonItems.map(item =&gt; item[prop.key] || '-');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>//
Проверяем, есть ли различия между значениями<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>const
hasDifferences = values.some((val, i, arr) =&gt; val !== arr[0]);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>tableHtml += `&lt;tr&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>tableHtml += `&lt;td&gt;${prop.label}&lt;/td&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>values.forEach(value =&gt; {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>const cellClass = hasDifferences ? 'difference' : '';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>//
Обрезаем длинное описание для мобильных устройств<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>let displayValue = value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>if
(prop.key === 'description' &amp;&amp; window.innerWidth &lt; 768) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                       
</span>displayValue = value.length &gt; 50 ? value.substring(0, 50) + '...' :
value;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                    </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>tableHtml += `&lt;td
class=&quot;${cellClass}&quot;&gt;${displayValue}&lt;/td&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>tableHtml += `&lt;/tr&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>tableHtml
+= `&lt;/tbody&gt;&lt;/table&gt;`;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>comparisonResults.innerHTML = tableHtml;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>comparisonModal.style.display = 'block';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
закрытия модального окна сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
closeComparison() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>comparisonModal.style.display = 'none';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Назначение
обработчиков событий<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>       
</span>modelSelect.addEventListener('change', function() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>isSearchingBySKU = false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>updateFilterOptions();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>filterSKU();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>       
</span>androidSelect.addEventListener('change', function() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>isSearchingBySKU = false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>filterSKU();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>       
</span>scannerSelect.addEventListener('change', function() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>isSearchingBySKU = false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>filterSKU();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>       
</span>memorySelect.addEventListener('change', function() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
