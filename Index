selects = [androidSelect, scannerSelect, memorySelect, buttonsSelect,
handleSelect];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>selects.forEach(select =&gt; {<o:p></o:p></p>

