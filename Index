<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>&quot;close&quot;: &quot;<span lang=EN-US style='font-family:"Malgun Gothic",sans-serif;
mso-bidi-font-family:"Malgun Gothic"'>닫기</span>&quot;,<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>&quot;comparison_title&quot;: &quot;<span lang=EN-US style='font-family:
"Malgun Gothic",sans-serif;mso-bidi-font-family:"Malgun Gothic"'>제품</span><span
lang=EN-US> </span><span lang=EN-US style='font-family:"Malgun Gothic",sans-serif;
mso-bidi-font-family:"Malgun Gothic"'>비교</span>&quot;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>};<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Текущий
язык<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>let
currentLanguage = 'ru';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Флаг поиска
по SKU<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>let
isSearchingBySKU = false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Массив для
хранения выбранных для сравнения SKU<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>let
comparisonItems = [];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Состояние
видимости фильтров на мобильных<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>let
filtersVisible = false;<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
переключения видимости фильтров на мобильных<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
toggleFilters() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
filters = document.getElementById('filters');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>filtersVisible = !filtersVisible;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(filtersVisible) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>filters.classList.add('visible');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>document.querySelector('.mobile-filter-toggle').textContent = 'Скрыть
фильтры';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>} else {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>filters.classList.remove('visible');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>document.querySelector('.mobile-filter-toggle').textContent = 'Показать
фильтры';<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
смены языка<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
changeLanguage(lang) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>currentLanguage = lang;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>document.documentElement.lang = lang;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Обновляем все элементы с атрибутом data-translate<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
elements = document.querySelectorAll('[data-translate]');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>elements.forEach(element =&gt; {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>const
key = element.getAttribute('data-translate');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(translations[lang][key]) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>element.textContent = translations[lang][key];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>});<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>// Также
обновляем опции в выпадающих списках<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>updateSelectOptions();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Обновляем текст в поиске<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>document.getElementById('sku-search').placeholder =
translations[lang]['search_placeholder'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>document.querySelector('.search-box button').textContent =
translations[lang]['search_button'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>document.getElementById('search-info').textContent =
translations[lang]['search_info'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>document.getElementById('search-footer').textContent =
translations[lang]['search_footer'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Перефильтруем результаты, чтобы обновить язык<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(isSearchingBySKU) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>searchBySKU();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>} else {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>filterSKU();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>//
Обновляем кнопки сравнения<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>updateComparisonButtons();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
обновления опций в выпадающих списках<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
updateSelectOptions() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
selects = {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>'model-select': 'all_models',<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>'android-select': 'any_version',<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>'scanner-select': 'any_scanner',<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>'memory-select': 'any_memory',<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>'buttons-select': 'any_count',<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>'handle-select': 'any_type',<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>'lte-select': 'not_important',<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>'frost-select': 'not_important'<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>};<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>for (const
[selectId, optionKey] of Object.entries(selects)) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>const
select = document.getElementById(selectId);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>if
(select.options.length &gt; 0) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                   
</span>select.options[0].text = translations[currentLanguage][optionKey];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>                </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>// Особые
случаи для LTE и морозостойкости<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
lteSelect = document.getElementById('lte-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(lteSelect.options.length &gt; 1) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>lteSelect.options[1].text = translations[currentLanguage]['has_lte'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>lteSelect.options[2].text = translations[currentLanguage]['no_lte'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
frostSelect = document.getElementById('frost-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(frostSelect.options.length &gt; 1) {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>frostSelect.options[1].text =
translations[currentLanguage]['frost_resistant'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>frostSelect.options[2].text =
translations[currentLanguage]['not_frost_resistant'];<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Элементы
DOM<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
modelSelect = document.getElementById('model-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
androidSelect = document.getElementById('android-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
scannerSelect = document.getElementById('scanner-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
memorySelect = document.getElementById('memory-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
buttonsSelect = document.getElementById('buttons-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
handleSelect = document.getElementById('handle-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
lteSelect = document.getElementById('lte-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
frostSelect = document.getElementById('frost-select');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
resultsContainer = document.getElementById('results-container');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
skuSearchInput = document.getElementById('sku-search');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
comparisonPanel = document.getElementById('comparison-panel');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
comparisonItemsContainer = document.getElementById('comparison-items');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
comparisonModal = document.getElementById('comparison-modal');<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>const
comparisonResults = document.getElementById('comparison-results');<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
поиска по SKU<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
searchBySKU() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
searchTerm = skuSearchInput.value.trim();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>if
(searchTerm === '') {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>isSearchingBySKU = false;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>filterSKU();<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>return;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>}<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>isSearchingBySKU = true;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>// Ищем
точное совпадение SKU (без учета регистра)<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>const
foundSKU = skuData.filter(item =&gt; <o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>               
</span>item.sku.toLowerCase().includes(searchTerm.toLowerCase())<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span>);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>            </span><o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>displayResults(foundSKU, searchTerm);<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>}<o:p></o:p></p>

<p class=MsoNormal><o:p>&nbsp;</o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>// Функция
сброса фильтров<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>        </span>function
resetFilters() {<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>modelSelect.value = &quot;&quot;;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>androidSelect.value = &quot;&quot;;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>scannerSelect.value = &quot;&quot;;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>memorySelect.value = &quot;&quot;;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>buttonsSelect.value = &quot;&quot;;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>handleSelect.value = &quot;&quot;;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>lteSelect.value = &quot;&quot;;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
</span>frostSelect.value = &quot;&quot;;<o:p></o:p></p>

<p class=MsoNormal><span style='mso-spacerun:yes'>           
